{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "auto-gaming-maintenance",
  "version": "0.1.0",
  "identifier": "com.manec.gaming.optimizer",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Auto Gaming Maintenance",
        "width": 800,
        "height": 650,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 600,
        "minHeight": 500,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}